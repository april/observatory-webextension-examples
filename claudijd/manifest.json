{

  "description": "Claudijd's Observatory Web Extension",
  "manifest_version": 2,
  "name": "http-response-filter",
  "version": "1.0",
  "homepage_url": "https://github.com/mdn/webextensions-examples/tree/master/http-response",
  "icons": {
    "48": "pen.svg"
  },

  "permissions": [
    "webRequest", "webRequestBlocking", "*://*/*"
  ],

  "background": {
    "scripts": ["jquery.min.js", "background.js"]
  },

  "browser_action": {
    "default_icon": "icons/unknown.svg",
    "default_title": "Observatory Scan Results"
  },

  "applications": {
    "gecko": {
      "strict_min_version": "57.0a1"
    }
  }

}
